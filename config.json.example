{
    "level": {
    	"compliance": "2"
    },
    "profile": {
    	"services": {
		    "enable": [
		    	"palette",
			"blurhash",
		    	"imagehash",
			"pigo"
		    ]
	}
    },
    "palette": {
    	"extruder": { "name": "vibrant", "count": 5 },
    	"grid": { "name": "euclidian" },
	"palette": [
		    { "name": "crayola" },
		    { "name": "css4" }
        ]
    },
    "blurhash": { "x": 8, "y": 8, "size": 200 },
    "imagehash": {},
    "pigo": {
	"cascade_source":"file:///path/to/go-iiif/var/pigo/cascade/",
	"cascade_file":"facefinder",
	"min_size":20,
	"max_size":1000,
	"shift_factor":0.1,
	"scale_factor":1.1
    },    
    "graphics": {
	"source": { "name": "native" }    
    },
    "features": {
	"enable": {
	    "size": [ "max" ],
	    "format": [ "webp", "tif", "gif" ],
	    "rotation": [ "noAutoRotate" ]
	},
	"disable": {
	    "rotation": [ "rotationArbitrary" ],
	    "quality": [ "bitonal" ]
	},
	"append": {
	    "quality": {
		"dither": { "syntax": "dither", "required": false, "supported": true, "match": "^dither$" }
	    }
	}
    },
    "images": {
	"source": { "name": "Disk", "path": "example/images" },
	"cache": { "name": "Memory", "ttl": 300, "limit": 100 }
    },
    "derivatives": {
	"cache": { "name": "Disk", "path": "example/cache" }
    }
}
